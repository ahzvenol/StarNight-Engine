{
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "stylelint.validate": ["css", "less", "postcss", "scss", "sass"],
    "[scss]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.stylelint": "explicit",
        }
    }
}
